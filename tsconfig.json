{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "module": "ES6",
    "skipLibCheck": true,
    "noEmit": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "types": ["node"],
    "jsx": "react-native",
    "lib": ["ES6", "ES2017"],
    "target": "ES5"
  },
  "exclude": ["./dist", "./node_modules"]
}
